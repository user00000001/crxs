{
  "name": "Chapter 4 (MV3) Service Worker",
  "version": "1.0",
  "manifest_version": 3,
  "description": "This is Chapter 4",
  "icons": {
    "128": "icons/icon_128.png",
    "48": "icons/icon_48.png",
    "16": "icons/icon_16.png"
  },
  "action": {
    "default_title": "I am Chapter 4",
